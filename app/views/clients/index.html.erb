<h1>My Clients</h1>

<div class="clients-list">

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
  </thead>  
  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td>
          <%= client.email %>
        </td>
        <td>
          <%= client.first_name %>
        </td>
        <td>
          <%= client.last_name %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>