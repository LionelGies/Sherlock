
<% content_for :head do %>
  <META NAME="ROBOTS" CONTENT="NOINDEX, NOFOLLOW"/>
<% end %>


    <div id="torn_paper_header" class="line">
      <div class="container_12">
        <div class="grid_12">
          <h1>
            CheckOut <em>SherlockDocs</em> Plans &amp; Pricing
          </h1>
          
          <p>Create Your Reports in Half the Time. Users have access to their reports forever.</p>
        </div>
      </div>
      
      <div class="torn_paper"></div>
    </div>
    
    <div id="content" class="line">
      <div class="container_12">
        
        <div id="pricing" class="line">
        
